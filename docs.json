
{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "Chelsea - Documentation",
  "colors": {
    "primary": "#000000",
    "light": "#333333",
    "dark": "#666666"
  },
  "favicon": "/favicon.svg",
  "fonts": {
    "family": "Ubuntu Mono",
    "weight": 400
  },
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "overview",
              "installation",
              "core-concepts"
            ]
          }
        ]
      },
      {
        "tab": "Tutorials",
        "groups": [
          {
            "group": "Tutorials",
            "pages": [
              "tutorials/your-first-project",
              "tutorials/parallel-web-testing",
              "tutorials/managing-branches",
              "tutorials/working-with-rootfs"
            ]
          }
        ]
      },
      {
        "tab": "CLI Reference",
        "groups": [
          {
            "group": "Project Commands",
            "pages": [
              "cli-reference/init",
              "cli-reference/up"
            ]
          },
          {
            "group": "Branch Commands",
            "pages": [
              "cli-reference/branch",
              "cli-reference/checkout"
            ]
          },
          {
            "group": "VM Commands",
            "pages": [
              "cli-reference/connect",
              "cli-reference/kill"
            ]
          },
          {
            "group": "Auth & Config",
            "pages": [
              "cli-reference/login",
              "cli-reference/configuration"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoints",
            "openapi": ["api-reference/openapi.json"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/hdresearch/chelsea",
          "icon": "github"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@yourproject.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/hdresearch/chelsea"
      }
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/hdresearch/chelsea"
    }
  },
  "api": {
    "playground": {
      "display": "interactive"
    }
  }
}